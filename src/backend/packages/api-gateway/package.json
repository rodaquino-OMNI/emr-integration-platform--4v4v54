    "axios-retry": "3.5.0",
    "opossum": "6.4.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dayjs": "1.11.9",
    "dotenv": "16.3.1",
    "express": "^4.18.0",
    "express-http-proxy": "^1.6.3",
    "express-oauth2-jwt-bearer": "^1.5.0",
    "express-rate-limit": "6.7.0",
    "express-sanitizer": "^1.0.6",
    "helmet": "^7.0.0",
    "http-errors": "2.0.0",
