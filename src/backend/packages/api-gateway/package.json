    "mockdate": "^3.0.5",
    "nock": "^13.3.0",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "supertest": "^6.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
