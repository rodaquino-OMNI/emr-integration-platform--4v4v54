  "version": "1.0.0",
  "private": true,
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf dist coverage",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn src/server.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
